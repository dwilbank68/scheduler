<%= div_for unit, class:'machine-container' do %>
    <div class="unit group">
      <header class="quicksand"><%= unit.screenName %><span id="show-queue">></span></header>
      <img src="data:image/jpeg;base64," alt="unit.screenName"/>
      <div class="numbers countdown" data-duration="<%= unit.total_duration %>">
        <div class="val remaining">
          <li class="hours"></li>   <li class="colon1">:</li>
          <li class="minutes"></li> <li class="colon2">:</li>
          <li class="secs"></li>
          <li class="time-label remaining">remaining</li>
        </div>
        <div class="val complete">
          <li class="hours"></li>   <li class="colon1">:</li>
          <li class="minutes"></li> <li class="colon2">:</li>
          <li class="secs"></li>
          <li class="time-label completed">est time of</li>
          <li class="time-label completed2">completion</li>
        </div>
      </div>

      <section class="user-queue"><!--user queue-->
        <ul><%= render unit_users(unit) %></ul>
      </section> <!-- end user queue -->

    </div> <!-- end unit -->

<% end %>

<%= minimal_form_for(@user, html: { class:'form-vertical' }) do |f| %>
    <%= f.error_notification %>
    <%= f.input :name %>
    <%= f.input :phone %>
    <%= f.input :SMS %>
    <%= f.input :email %>
    <%#= f.input :start_time %>


    <%= f.input :duration %>
    <%= f.input :note %>
    <%= f.error :base %>
    <%= f.button :submit %>
<% end %>

